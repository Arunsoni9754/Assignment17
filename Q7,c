#include<stdio.h>
#include<string.h>
int main()
{
    char s[20];
    int i,l=0,k=0,j=0;
    printf("Enter the string ");
    scanf("%s",s);
    for(i=0;s[i]!='\0';i++)
    {
        if((s[i]>='a'&&s[i]<='z')||(s[i]>='A'&&s[i]<='Z'))
        {
            k++;
        }
        else if(s[i]>='0'&&s[i]<='9')
        {
            l++;
        }
        else
        {
            j++;
        }
    }
    printf("The alphabets are %d\n",k);
     printf("The numerics are %d\n",l);
    printf("The special symbols are %d",j);



}
